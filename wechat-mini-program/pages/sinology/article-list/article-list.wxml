<!--pages/sinology/article-list/article-list.wxml-->

<view class="articles-hd">{{title}}全集</view>

<view class="weui-cells weui-cells_after-title">
  <view wx:for="{{articles}}" wx:key="{{item.title}}" class="weui-cell" bindtap="goToDetail" data-select-title="{{item.title}}">
    <!-- Cell Left -->
    <view class="weui-cell__hd">
      <view class="sort-view">{{item.title}}</view>
    </view>
    <!-- Cell Middle Right -->
    <view class="weui-cell__bd letter-font"></view>
    <view class="weui-cell__ft letter-font">{{item.author}}</view>
  </view>
</view>
